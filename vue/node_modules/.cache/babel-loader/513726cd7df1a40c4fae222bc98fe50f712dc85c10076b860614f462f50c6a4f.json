{"ast":null,"code":"import { extend } from \"vee-validate\";\nimport { required, email, confirmed } from \"vee-validate/dist/rules\";\nextend(\"email\", email);\nextend(\"required\", required);\nextend(\"confirmed\", confirmed);\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\"\n    };\n  },\n  methods: {\n    submit() {\n      alert(\"Form has been submitted!\");\n    }\n  }\n};","map":{"version":3,"names":["extend","required","email","confirmed","data","password","confirmPassword","methods","submit","alert"],"sources":["src/components/Dashboard/Views/Forms/ValidationForms/RegisterForm.vue"],"sourcesContent":["<template>\n  <ValidationObserver v-slot=\"{ handleSubmit }\">\n    <form @submit.prevent=\"handleSubmit(submit)\">\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <h4 class=\"card-title\">\n            Register Form\n          </h4>\n        </div>\n        <div class=\"card-body\">\n          <div class=\"form-group\">\n            <label>Email address</label>\n            <ValidationProvider\n              name=\"email\"\n              rules=\"required|email\"\n              v-slot=\"{ passed, failed }\"\n            >\n              <fg-input  type=\"email\"\n                         :error=\"failed ? 'The Email field is required': null\"\n                         :hasSuccess=\"passed\"\n                         name=\"email\"\n                         v-model=\"email\">\n              </fg-input>\n            </ValidationProvider>\n          </div>\n          <div class=\"form-group\">\n            <label>Password</label>\n            <ValidationProvider\n              vid=\"confirmation\"\n              rules=\"required\"\n              v-slot=\"{ passed, failed }\"\n            >\n              <fg-input  type=\"password\"\n                         :error=\"failed ? 'The Password field is required': null\"\n                         :hasSuccess=\"passed\"\n                         name=\"password\"\n                         v-model=\"password\">\n              </fg-input>\n            </ValidationProvider>\n          </div>\n          <div class=\"form-group\">\n            <label>Confirm Password</label>\n            <ValidationProvider\n              rules=\"required|confirmed:confirmation\"\n              v-slot=\"{ passed, failed }\"\n            >\n              <fg-input  type=\"password\"\n                         :error=\"failed ? 'The Confirm field is required': null\"\n                         :hasSuccess=\"passed\"\n                         name=\"confirm\"\n                         v-model=\"confirmPassword\">\n              </fg-input>\n            </ValidationProvider>\n          </div>\n        </div>\n        <div class=\"card-footer text-right\">\n          <p-checkbox class=\"pull-left\">Subscribe to newsletter</p-checkbox>\n          <p-button type=\"info\" native-type=\"submit\">Register</p-button>\n        </div>\n      </div>\n    </form>\n  </ValidationObserver>\n</template>\n<script>\nimport { extend } from \"vee-validate\";\nimport { required, email, confirmed } from \"vee-validate/dist/rules\";\n\nextend(\"email\", email);\nextend(\"required\", required);\nextend(\"confirmed\", confirmed);\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\"\n    };\n  },\n  methods: {\n    submit() {\n      alert(\"Form has been submitted!\");\n    }\n  }\n}\n</script>\n<style>\n</style>\n"],"mappings":"AAgEA,SAAAA,MAAA;AACA,SAAAC,QAAA,EAAAC,KAAA,EAAAC,SAAA;AAEAH,MAAA,UAAAE,KAAA;AACAF,MAAA,aAAAC,QAAA;AACAD,MAAA,cAAAG,SAAA;AAEA;EACAC,KAAA;IACA;MACAF,KAAA;MACAG,QAAA;MACAC,eAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACAC,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}